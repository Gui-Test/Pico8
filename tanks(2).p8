-->8
--player

function player_make()
p={}
p.x = 6
p.y = 6
p.sped = 2
p.sprite = 1
end

function player_draw()
spr(p.sprite,p.x,p.y)
end

function player_move()
		
		--[[gambiarra pra ser igual
		battle city]]--
  if ((btn(⬅️)) and (btn(⬆️))) or 
  ((btn(⬅️)) and (btn(⬇️))) then 
  p.x -= p.sped
  elseif ((btn(➡️)) and (btn(⬆️))) or 
  ((btn(➡️)) and (btn(⬇️))) then
  p.x += p.sped
  
  --movimento normal
  elseif (btn(⬅️)) then 
  p.x -= p.sped 
  elseif (btn(➡️)) then
  p.x += p.sped
  elseif (btn(⬆️)) then 
  p.y -= p.sped 
  elseif (btn(⬇️)) then
  p.y += p.sped 
  end
   
end
